		"@storybook/react": "^8.6.12",
		"@storybook/react-vite": "^8.6.12",
		"@storybook/test": "^8.6.12",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^4.6.0",
		"playwright": "^1.57.0",
		"storybook": "^8.6.12",
		"typescript": "^5.9.3",
		"vite": "^6.3.5"
