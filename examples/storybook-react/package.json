    "pixelguard:test": "npm run pixelguard -- test",
    "pixelguard:update": "npm run pixelguard -- test --update",
    "pixelguard:list": "npm run pixelguard -- list"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
