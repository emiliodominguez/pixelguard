{
  "name": "pixelguard-example-storybook-react",
  "version": "0.1.0",
  "private": true,
  "description": "Example Storybook React project for testing Pixelguard",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
